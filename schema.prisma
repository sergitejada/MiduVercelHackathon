generator client {
  provider = "prisma-client-js"
}
 
datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}
 
model Gifts {
  id        	Int      @id @default(autoincrement())
  name      	String
  description String
  createdAt 	DateTime @default(now())
}