generator client {
  provider = "prisma-client-js"
	output   = "./generated/client"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Gifts {
  id        	Int      @id @default(autoincrement())
  name      	String
  description String
  images 			String
	createdAt 	DateTime @default(now())
}
